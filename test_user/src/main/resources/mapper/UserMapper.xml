<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xyu.user.mapper.UserMapper">
    <select id="alluser" resultType="com.xyu.user.domain.User">
        select * from tb_user
    </select>
    <select id="oneuser" resultType="com.xyu.user.domain.User">
        select * from tb_user where id=#{userid}
    </select>
</mapper>